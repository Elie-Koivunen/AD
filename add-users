https://blog.netwrix.com/2018/06/07/how-to-create-new-active-directory-users-with-powershell/
http://techgenix.com/creating-active-directory-accounts-using-powershell/ 

new-aduser -name "labuser1" -GivenName "labuser1" -Surname "labuser1" -SamAccountname "labuser1" -userprincipalname "labuser1@lab.scape" -ChangePasswordAtLogon $false -AccountPassword (ConvertTo-SecureString "1#Password" -AsPlainText -force) -passThru -enabled $true
new-aduser -name "labuser2" -GivenName "labuser2" -Surname "labuser2" -SamAccountname "labuser2" -userprincipalname "labuser2@lab.scape" -ChangePasswordAtLogon $false -AccountPassword (ConvertTo-SecureString "1#Password" -AsPlainText -force) -passThru -enabled $true
