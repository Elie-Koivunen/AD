new-adgroup -name labusers -groupscope global
Add-ADGroupMember -Identity "labusers" -Members labuser1,labuser2,labuser3,labuser4,labuser5

new-adgroup -name appusers -groupscope global
Add-ADGroupMember -Identity "appusers" -Members appuser1,appuser2,appuser3,appuser4,appuser5

new-adgroup -name admins-cos01 -groupscope global
Add-ADGroupMember -Identity "admins-cos01" -Members zoneadmin1,labadmin1,labuser1

new-adgroup -name admins-cos02 -groupscope global
Add-ADGroupMember -Identity "admins-cos02" -Members zoneadmin2,labadmin2,labuser2

new-adgroup -name admins-cos03 -groupscope global
Add-ADGroupMember -Identity "admins-cos03" -Members zoneadmin3,labadmin3,labuser3

new-adgroup -name admins-lab -groupscope global
Add-ADGroupMember -Identity "admins-lab" -Members labadmin1,labadmin2,labadmin3,labadmin4,labadmin5

new-adgroup -name admins-zone -groupscope global
Add-ADGroupMember -Identity "admins-zone" -Members zoneadmin1,zoneadmin2,zoneadmin3,zoneadmin4,zoneadmin5

new-adgroup -name admins-homedir -groupscope global
Add-ADGroupMember -Identity "admins-homedir" -Members homeadmin1,homeadmin2,homeadmin3,homeadmin4,homeadmin5

new-adgroup -name admins-groupdir -groupscope global
Add-ADGroupMember -Identity "admins-groupdir" -Members groupadmin1,groupadmin2,groupadmin3,groupadmin4,groupadmin5

new-adgroup -name admins-appdir -groupscope global
Add-ADGroupMember -Identity "admins-appdir" -Members appadmin1,appadmin2,appadmin3,appadmin4,appadmin5

new-adgroup -name admins-pod01 -groupscope global
Add-ADGroupMember -Identity "admins-pod01" -Members podadmin1,podadmin2

new-adgroup -name admins-pod02 -groupscope global
Add-ADGroupMember -Identity "admins-pod02" -Members podadmin2,podadmin3

new-adgroup -name admins-pod03 -groupscope global
Add-ADGroupMember -Identity "admins-pod03" -Members podadmin3,podadmin4

new-adgroup -name admins-pod04 -groupscope global
Add-ADGroupMember -Identity "admins-pod04" -Members podadmin4,podadmin5
